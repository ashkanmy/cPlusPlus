#include <iostream>

using namespace std;


template<typename T>
void PassArrayAsReference(T& myArray)
{
    cout << myArray[0]
         << myArray[1] 
         << myArray[2] 
         << myArray[3]
         << myArray[4]
         << myArray[5]
         << myArray[6]
         << myArray[7]
         << myArray[8]
         << myArray[9];
    
}

int main()
{
    const char arr[] = "A. M. Yar.";
    PassArrayAsReference(arr);
    
    return 0;
}
